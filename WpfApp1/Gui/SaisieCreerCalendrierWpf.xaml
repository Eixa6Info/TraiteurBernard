<Window x:Class="TraiteurBernardWPF.Gui.SaisieCreerCalendrierWpf"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TraiteurBernardWPF.Gui"
      mc:Ignorable="d"
      xml:lang="fr"
      Title="Calendrier" Width="405" Height="497" Closed="Window_Closed" Loaded="Window_Loaded" UseLayoutRounding="True" RenderOptions.BitmapScalingMode="NearestNeighbor" >

   
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="331*"/>
            <ColumnDefinition Width="13*"/>
        </Grid.ColumnDefinitions>


        <Calendar x:Name="calendar" Margin="56,30,42,0" SelectionMode="MultipleRange" UseLayoutRounding="True" RenderOptions.BitmapScalingMode="HighQuality" Foreground="#DD000000" VerticalAlignment="Top" MouseDoubleClick="calendar_MouseDoubleClick" Height="416" >
        </Calendar>
        


    </Grid>
    
    
</Window>
